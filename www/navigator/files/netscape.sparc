XKEYSYMDB=@netscape_home@
XNLSPATH=@netscape_home@/nls
MOZILLA_HOME=@netscape_home@
export XKEYSYMDB XNLSPATH MOZILLA_HOME
exec $MOZILLA_HOME/netscape "$@"
