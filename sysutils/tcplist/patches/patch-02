  #define LSOFCMD  "lsof -HPsli TCP | awk '{print $3,$NF}' | sort | uniq"
  #else /* !OLDLSOF */
  /* These use the current (as of 3.62W, at least) parameters for lsof */
! #define LSOFCMD  "lsof -nPsli TCP | awk '{print $3,$(NF - 1)}' | sort | uniq"
  #endif /* !OLDLSOF */
  #endif /* LSOFCMD */
  
