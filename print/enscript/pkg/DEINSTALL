# Pre/post-uninstallation setup of enscript

echo
echo "+---------------"
echo "| To completely deinstall the enscript package you need to perform"
echo "| this steps as root:"
echo "|"
echo "|           rm -f ${SYSCONFDIR}/enscript.cfg"
echo "+---------------"
echo

exit 0
