-install-data-am: install-hackDATA
-install-data: install-data-am
+install-data-am:
+install-data:
 
-install-am: all-am
-	@$(MAKE) $(AM_MAKEFLAGS) install-exec-am install-data-am
-install: install-am
+install-am:
+install:
 uninstall-am: uninstall-hackDATA
 uninstall: uninstall-am
 all-am: Makefile $(DATA)
