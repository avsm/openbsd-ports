   if test "x$FOUND_CDPARANOIA" = "xyes";
   then
     AC_CHECK_LIB(cdda_paranoia,
-                 paranoia_cachemodel_size,
+                 paranoia_init,
                  : ,
-                 HAVE_CDPARANOIA=no)
+                 HAVE_CDPARANOIA=no, -lcdda_interface -lm)
   fi
 
   AC_SUBST(CDPARANOIA_LIBS)
