 inline u2 Bytes::swap_u2(u2 x) 
 {
+#ifdef __OpenBSD__
+  return swap16(x);
+#else
   return __bswap16(x);
+#endif
 }
 
 inline u4 Bytes::swap_u4(u4 x) 
 {
+#ifdef __OpenBSD__
+  return swap32(x);
+#else
   return __bswap32(x);
+#endif
 }
 
 inline u8 Bytes::swap_u8(u8 x) 
 {
+#ifdef __OpenBSD__
+  return swap64(x);
+#else
   return __bswap64(x);
+#endif
 }
