*** 14,20 ****
  #
  
  sort +2rn arp.dat | \
!     awk -e 'NF == 4 { print }' | \
      awk -f p.awk | \
      egrep -v '\.[0-9][0-9]*$' | \
      sed -e 's/	.*	/	/' | \
