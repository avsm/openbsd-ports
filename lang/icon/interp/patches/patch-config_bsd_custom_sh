+#!/bin/sh
+#
+#  custom setup script for openbsd
+
+SRC=../../../src
+ARCH=`uname -m`
+
+if [ -f define.$ARCH ]; then
+        cat define.$ARCH >>$SRC/h/define.h
+fi
