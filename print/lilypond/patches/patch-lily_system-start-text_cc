+      Interval ix (p_extent_x[LEFT], p_extent_x[RIGHT] + right_padding);
+      Box box (ix, p->extent (Y_AXIS));
       Stencil *aligned_p = new Stencil (box, p->expr ());
       return *aligned_p;
