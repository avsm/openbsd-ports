+#ifndef swap16
 #define swap16(x) \
       ((((x) >> 8) & 0xff) | (((x) & 0xff) << 8))
+#endif
  
+#ifndef swap32
 #define swap32(x) \
       ((((x) & 0xff000000) >> 24) | (((x) & 0x00ff0000) >>  8) \
      | (((x) & 0x0000ff00) <<  8) | (((x) & 0x000000ff) << 24))
+#endif
+#ifndef swap64
 #define swap64(x) \
