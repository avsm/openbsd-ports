       Interval padding (0.0, max (0.0, indent - p_extent_x.length ()));
       Real right_padding = padding.length () - padding.linear_combination (align_x);
-      Box box (Interval (p_extent_x[LEFT], p_extent_x[RIGHT] + right_padding),
