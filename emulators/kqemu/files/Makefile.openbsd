# $OpenBSD: Makefile.openbsd,v 1.4 2008/04/29 01:51:25 todd Exp $

NOMAN = noman
CFLAGS += -Wall
COMMON = kqemu-mod-${MACHINE_ARCH:S/amd64/x86_64/}.o
SRCS = kqemu-openbsd.c
OBJS = ${COMMON}
LKM = kqemu
COMBINED = kqemu.o

.include <bsd.lkm.mk>

${COMMON}:
	gmake -C common all

clean:
	rm -f $(COMMON)
	gmake -C common clean
