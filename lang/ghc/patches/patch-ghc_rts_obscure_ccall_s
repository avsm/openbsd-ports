+++ ghc/rts/obscure_ccall.s	Thu Jul 10 14:48:47 2003
@@ -0,0 +1,6 @@
+        .text
+        .globl __obscure_ccall_ret_code
+
+__obscure_ccall_ret_code:
+        addl $4, %esp
+        ret
