 	-DLIBRARY_SUBMINOR_VERSION=${SUBMINOR_VERSION} \
 
-ifeq ($(patsubstr GNU/%,glibc,$(shell uname -o)),glibc)
-ADDITIONAL_CPPFLAGS += \
-        -DHAVE_STRNDUP
-endif
