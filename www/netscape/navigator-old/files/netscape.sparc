#!/bin/sh
# $OpenBSD: netscape.sparc,v 1.3 2000/09/01 04:24:43 brad Exp $
#
# Wrapper for Fortified Netscape Navigator program.

MOZILLA_HOME=@netscape_home@ ; export MOZILLA_HOME
XKEYSYMDB=$MOZILLA_HOME/XKeysymDB
XNLSPATH=$MOZILLA_HOME/nls
export XKEYSYMDB XNLSPATH
exec $MOZILLA_HOME/netscape "$@"
