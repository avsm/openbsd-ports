	mkdir -p ${DESTDIR}${DOCDIR}
	${INSTALL} -c -o ${BINOWN} -g ${BINGRP} -m 444 ${FILES} \
		${DESTDIR}${DOCDIR}

.include <bsd.prog.mk>
