 	}
 
 	if (time (NULL) < audioscrobbler->priv->handshake_next) {
-		rb_debug ("Too soon; time=%lu, handshake_next=%lu",
+		rb_debug ("Too soon; time=%d, handshake_next=%d",
 			  time (NULL),
 			  audioscrobbler->priv->handshake_next);
 		return FALSE;
