-    else PROCESSOR=`uname -m`
-    fi
-  fi
-  OS=`uname -s`
-  REL=`uname -r`
-else
-  if arch >/dev/null 2>&1
-  then PROCESSOR=`arch`
-  else
-    if machine >/dev/null 2>&1
-    then PROCESSOR=`machine`
-    else PROCESSOR='unknown'
-    fi
-  fi
-  OS=unknown
