@sample ${SYSCONFDIR}/pdnsd.conf
@owner _pdnsd
@group _pdnsd
@sample /var/pdnsd/
@exec dd if=/dev/zero of=/var/pdnsd/pdnsd.cache bs=1 count=4 2>/dev/null
@exec chown _pdnsd:_pdnsd /var/pdnsd/pdnsd.cache
@extraunexec rm -rf /var/pdnsd
