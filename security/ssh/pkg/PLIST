man/man1/scp.1
man/man1/ssh-add.1
man/man1/ssh-agent.1
man/man1/ssh-keygen.1
man/man1/ssh.1
@exec ln -fs %f %B/slogin.1
@unexec rm -f %B/slogin.1
man/man8/sshd.8
sbin/sshd
@exec if [ ! -f /etc/ssh_host_key ]; then echo "Generating a secret host key.." ; %D/bin/ssh-keygen -N "" -f /etc/ssh_host_key; fi
