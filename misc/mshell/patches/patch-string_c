 #define strchr	index
 #endif
 
+char *strsave(char *);
 
+char *
+gets (char *s)
+{
+    char *x = fgets (s, WORDLEN, stdin);
+    if (x) {
+	char *p = strchr (x, '\n');
+	if (p) 
+	    *p = 0;
+    }
+    return x;
+}
